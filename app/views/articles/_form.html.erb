<%= render 'form_error'%>
<%= form_with(model: @article, local: true) do |f| %>

  <p>
    <%= f.label :title%>
    <%= f.text_field :title%>
  </p>
  <p>
    <%= f.label :description%>
    <%= f.text_area :description%>
  </p>
  <p>
    <%= f.label :price%>
    <%= f.number_field :price%>
  </p>
  <p>
    <%= f.label :quantity%>
    <%= f.number_field :quantity%>
  </p>
  <p>
    <%= f.submit%>
  </p>
<% end %>